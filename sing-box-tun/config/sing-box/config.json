{
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "independent_cache": true,
        "rules": [
            {
                "outbound": "any",
                "server": "local"
            },
            {
                "query_type": [
                    "AAAA",
                    "A"
                ],
                "server": "fakeip"
            }
        ],
        "servers": [
            {
                "address": "https://1.1.1.2/dns-query",
                "tag": "remote"
            },
            {
                "address": "local",
                "detour": "direct",
                "tag": "local"
            },
            {
                "address": "fakeip",
                "tag": "fakeip"
            }
        ],
        "strategy": "prefer_ipv6"
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": true
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "xd",
            "external_ui_download_detour": "direct",
            "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/gh-pages.zip"
        }
    },
    "inbounds": [
        {
            "address": [
                "fdfe:dcba:9876::1/126",
                "172.19.0.1/30"
            ],
            "auto_route": true,
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "localhost",
                    "server_port": 7890
                }
            },
            "stack": "system",
            "strict_route": true,
            "type": "tun"
        },
        {
            "listen": "::",
            "listen_port": 7890,
            "type": "mixed"
        }
    ],
    "log": {
        "level": "fatal"
    },
    "outbounds": [
        {
            "interrupt_exist_connections": true,
            "outbounds": [
                "auto",
                "manual",
                "direct"
            ],
            "tag": "default",
            "type": "selector"
        },
        {
            "outbounds": [],
            "tag": "auto",
            "type": "urltest",
            "url": "https://dns.cloudflare.com/dns-query"
        },
        {
            "interrupt_exist_connections": true,
            "outbounds": [],
            "tag": "manual",
            "type": "selector"
        },
        {
            "interrupt_exist_connections": true,
            "outbounds": [
                "true-direct"
            ],
            "tag": "direct",
            "type": "selector"
        },
        {
            "tag": "true-direct",
            "type": "direct"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "rule_set": [
            {
                "download_detour": "direct",
                "format": "binary",
                "tag": "block",
                "type": "remote",
                "url": "https://cdn.jsdelivr.net/gh/demarcush/breakfree@master/release/rule-sets/block.srs"
            },
            {
                "download_detour": "direct",
                "format": "binary",
                "tag": "direct",
                "type": "remote",
                "url": "https://cdn.jsdelivr.net/gh/demarcush/breakfree@master/release/rule-sets/direct-cn.srs"
            },
            {
                "download_detour": "direct",
                "format": "binary",
                "tag": "proxy",
                "type": "remote",
                "url": "https://cdn.jsdelivr.net/gh/demarcush/breakfree@master/release/rule-sets/proxy-cn.srs"
            },
            {
                "download_detour": "direct",
                "format": "binary",
                "tag": "nsfw",
                "type": "remote",
                "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-nsfw.srs"
            }
        ],
        "rules": [
            {
                "action": "sniff"
            },
            {
                "action": "hijack-dns",
                "protocol": "dns"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "outbound": "direct",
                "protocol": "bittorrent"
            },
            {
                "action": "reject",
                "rule_set": "block"
            },
            {
                "outbound": "default",
                "rule_set": "proxy"
            },
            {
                "outbound": "direct",
                "rule_set": "direct"
            }
        ]
    }
}
